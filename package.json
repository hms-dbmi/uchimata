    "@types/three": "^0.180.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.30.0",
    "@preact/signals-core": "^1.12.1",
    "@uwdata/flechette": "^2.2.3",
    "chroma-js": "^3.1.2",
    "gl-matrix": "3.4.3",
