{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/main.ts"],
  "out": "docs-api",
  "plugin": ["typedoc-plugin-markdown"],
  "outputFileStrategy": "modules",
  "mergeReadme": true,
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "sort": ["source-order"]
}
